                    "body_content": [
                        {
                            "explanation": "to sit or stand with your legs on either side of something: ",
                            "examples": [
                                "He pulled on his helmet and straddled the motorbike."
                            ]
                        },
                        {
                            "explanation": "Something that straddles a line, such as a border or river, exists on each side of it or goes across it: ",
                            "examples": [
                                "Our farm straddles the railway line."
                            ]
                        },
                        {
                            "explanation": "to combine different styles or subjects: ",
                            "examples": [
                                "It's described as a new kind of dance music which straddles jazz and soul."
                            ]
                        },
                        {
                            "explanation": "to be unable to decide which of two opinions about a subject is better and so partly support both opinions: ",
                            "examples": [
                                "It's not the first time this year that the president has been accused of straddling an issue."
                            ]
                        }
